      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "# Replace ham with 0 and spam 1\n",
        "emailSMSData = emailSMSData.replace('spam', 1)\n",
        "emailSMSData = emailSMSData.replace('ham', 0)"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
